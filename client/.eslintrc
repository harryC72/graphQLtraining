{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "linebreak-style": 0,
  "global-require": 0,
  "eslint linebreak-style": [0, "error", "windows"],
  "no-console": "off",
  "react/destructuring-assignment": "never",
}
}